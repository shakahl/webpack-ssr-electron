{
  "name": "webpack-ssr-electron",
  "description": "Vuetify electron template with webpack ssr ",
  "author": "Kylart",
  "version": "0.0.1",
  "scripts": {
    "dev": "electron server",
    "start": "NODE_ENV=production electron server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules"
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "es6-promise": "4.2.8",
    "express": "4.17.3",
    "extract-text-webpack-plugin": "2.1.2",
    "lru-cache": "4.1.5",
    "serialize-javascript": "3.1.0",
    "serve-favicon": "2.5.0",
    "vue": "2.7.0",
    "vue-router": "2.8.1",
    "vue-server-renderer": "2.7.0",
    "vuetify": "0.17.7",
    "vuex": "2.5.0",
    "vuex-router-sync": "4.3.2"
  },
  "devDependencies": {
    "autoprefixer": "7.2.6",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
