    "pug": "3.0.1",
    "rimraf": "2.7.1",
    "script-ext-html-webpack-plugin": "1.8.8",
    "style-loader": "0.23.1",
    "stylus": "0.56.0",
    "stylus-loader": "3.0.2",
    "sw-precache-webpack-plugin": "0.9.2",
    "url-loader": "0.6.2",
    "vue-loader": "12.2.2",
